<launch>
  
  <param name="robot_description" textfile="$(find robot_simulation)/urdf/robot_simulation.urdf" />
  <node name="servo_drive" pkg="arm_control" type="servos_drive.py" output="screen"/>
  <node name="Arm_control_server" pkg="arm_control" type="Arm_control_server.py" output="screen"/>
  <node pkg="tf" type="static_transform_publisher" name="link1_broadcaster" args="0.084 0 0.023 -1.57 0 -1.74533 base_link camera 100" />
  <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
    respawn="false" output="screen">
    <remap from="/joint_states" to="/robot_simulation/joint_states" />
  </node>

</launch>
